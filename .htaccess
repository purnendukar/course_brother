<FilesMatch "\.(js)">
    ForceType application/x-httpd-php

	php_value auto_prepend_file "/the/full/path/of/this/file/gzip-js.php"

</FilesMatch>

<FilesMatch "\.(css)">
    ForceType application/x-httpd-php

	php_value auto_prepend_file "/the/full/path/of/this/file/gzip-css.php"

</FilesMatch>